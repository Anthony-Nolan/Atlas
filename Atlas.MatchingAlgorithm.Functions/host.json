{
  "extensions": {
    "serviceBus": {
      "messageHandlerOptions": {
        "maxAutoRenewDuration": "2.00:30:00"
      }
    }
  },
  "functionTimeout": "2.00:00:00",
  "logging": {
    "logLevel": {
      "default": "Trace"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false,
        "excludedTypes": "Request;Event"
      },
      "snapshotConfiguration": {
        "isEnabled": true
      }
    }
  },
  "version": "2.0"
}