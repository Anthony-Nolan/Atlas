{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Event;Trace;Exception"
      }
    }
  },
  "extensions": {
    "serviceBus": {
      "maxConcurrentCalls": 1,
      "maxAutoLockRenewalDuration": "02:00:00"
    }
  },
  "functionTimeout": "01:30:00"
}