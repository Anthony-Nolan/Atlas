{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"updateMode": {
			"type": "string",
			"enum": [
				"diff",
				"full"
			],
			"description": "Differential or Full"
		},
		"donors": {
			"type": "array",
			"minItems": 1,
			"items": {
				"type": "object",
				"properties": {
					"recordId": {
						"type": "string"
					},
					"changeType": {
						"type": "string",
						"enum": [
							"D",
							"U",
							"N",
							"NU"
						],
						"description": "Deleted, Updated, New or New-or-Update (Upsert)"
					},
					"donorType": {
						"type": "string",
						"enum": [
							"D",
							"C",
							"B"
						],
						"description": "Donor, Cord or Banked"
					},
					"donPool": {
						"type": [
							"string",
							"null"
						],
						"description": "Physical location of the donor, used to denote which registry a donor belongs to."
					},
					"ethn": {
						"type": [
							"string",
							"null"
						],
						"description": "Ethnicity of the donor."
					},
					"hla": {
						"$ref": "#/definitions/hla"
					}
				},
				"required": [
					"recordId",
					"changeType",
					"donorType",
					"donPool",
					"ethn",
					"hla"
				],
				"additionalProperties": false
			}
		}
	},
	"required": [
		"updateMode",
		"donors"
	],
	"additionalProperties": false,
