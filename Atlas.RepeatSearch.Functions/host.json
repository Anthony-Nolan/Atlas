{
  "version": "2.0",
  "extensions": {
    "serviceBus": {
      "maxConcurrentCalls": 3,
      "maxAutoLockRenewalDuration": "2.00:30:00"
    }
  },
  "functionTimeout": "2.00:00:00",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Event;Trace;Exception"
      },
      "snapshotConfiguration": {
        "isEnabled": true
      }
    }
  }
}