{
  "version": "2.0",
  "extensions": {
    "serviceBus": {
      "maxConcurrentCalls": 1,
      "maxAutoLockRenewalDuration": "02:00:00"
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Event;Trace;Exception"
      },
      "snapshotConfiguration": {
        "isEnabled": true
      }
    }
  }
}