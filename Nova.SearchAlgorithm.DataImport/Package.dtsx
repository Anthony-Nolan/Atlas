            <component
              refId="Package\Import Donor Table\Solar Donors"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="Solar Donors"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
-- DONOR INFO
d.donor_id,
CASE
  WHEN d.donor_type LIKE '%Cord' THEN 'Cord'
  ELSE 'Adult' 
  END AS donor_type,
CASE 
  WHEN d.donor_type = 'ANBMT' THEN 'AN'
  ELSE REPLACE(d.donor_type,'Cord','') 
  END AS donor_registry_code,
a_1.hla_name as A_1, a_2.hla_name as A_2,
b_1.hla_name as B_1, b_2.hla_name as B_2,
c_1.hla_name as C_1, c_2.hla_name as C_2,
drb1_1.hla_name as DRB1_1, drb1_2.hla_name as DRB1_2,
dqb1_1.hla_name as DQB1_1, dqb1_2.hla_name as DQB1_2
FROM DRS.DR_DONORS d
JOIN DRS.DR_DEFINITIVE_HLA_TYPE h
ON d.donor_id = h.donor_patient_id
LEFT JOIN DRS.DR_CORDS c
ON d.donor_id = c.donor_id
LEFT JOIN DRS.DR_ANTIGENS a_1 ON A_1_ID = a_1.antigen_id
LEFT JOIN DRS.DR_ANTIGENS a_2 ON A_2_ID = a_2.antigen_id
LEFT JOIN DRS.DR_ANTIGENS b_1 ON B_1_ID = b_1.antigen_id
LEFT JOIN DRS.DR_ANTIGENS b_2 ON B_2_ID = b_2.antigen_id
LEFT JOIN DRS.DR_ANTIGENS c_1 ON CW_1_ID = c_1.antigen_id
LEFT JOIN DRS.DR_ANTIGENS c_2 ON CW_2_ID = c_2.antigen_id
LEFT JOIN DRS.DR_ANTIGENS drb1_1 ON DRB1_1_ID = drb1_1.antigen_id
LEFT JOIN DRS.DR_ANTIGENS drb1_2 ON DRB1_2_ID = drb1_2.antigen_id
LEFT JOIN DRS.DR_ANTIGENS dqb1_1 ON DQB1_1_ID = dqb1_1.antigen_id
LEFT JOIN DRS.DR_ANTIGENS dqb1_2 ON DQB1_2_ID = dqb1_2.antigen_id
WHERE
  -- ONLY ACTIVE
  ((d.donor_status_type = 'Active' AND d.donor_type IN ('DKMS','NHSBT','ANBMT','WBS')) OR
   (c.donor_status_type != 'Closed' AND d.donor_type IN ('NHSBTCord','ANCord','NMDPCord','FRANCECord') AND c.donor_id is not null))
  -- ONLY THOSE WITH AT LEAST THREE LOCUS TYPE
